        "description": "API for BnB app: login, regions, motels, rooms, booking, chat. Protected routes require Bearer token (Sanctum)."
    },
    "servers": [
        {
            "url": "http://localhost:8000/api",
            "description": "Local"
        },
        {
            "url": "https://bnb.co.tz/api",
            "description": "Production"
        }
